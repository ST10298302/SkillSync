{
  "expo": {
    "name": "SkillSyncApp",
    "slug": "SkillSyncApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/skillsync-logo.png",
    "scheme": "skillsyncapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mavy132.SkillSyncApp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSFaceIDUsageDescription": "SkillSync uses Face ID to securely authenticate your identity and protect your skill data."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/skillsync-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.mavy132.SkillSyncApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "577801a1-420e-4622-b522-d1b52db1a8f2"
      }
    }
  }
}
